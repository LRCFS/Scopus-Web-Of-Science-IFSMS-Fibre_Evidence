@article{ ISI:000380720700012,
Author = {Blackie, Renee and Taylor, Duncan and Linacre, Adrian},
Title = {{DNA profiles from clothing fibers using direct PCR}},
Journal = {{FORENSIC SCIENCE MEDICINE AND PATHOLOGY}},
Year = {{2016}},
Volume = {{12}},
Number = {{3}},
Pages = {{331-335}},
Month = {{SEP}},
Abstract = {{We report on the successful use of direct PCR amplification of single
   fibers from items of worn clothing. Items of clothing were worn
   throughout the course of a day, with the individual commencing regular
   activities. Single fibers were taken from the cuff of the clothing at
   regular intervals and amplified directly. The same areas were subjected
   to tape-lifting, and also amplified directly for comparison. The NGM
   (TM) kit that amplifies 15 STR loci plus amelogenin was used. A total of
   35 single fiber samples were processed and analyzed from five items of
   clothing, with 81 \% of samples returning a profile of 14 alleles or
   more. All tape-lift samples amplified directly produced DNA profiles of
   15 alleles or more. The aim was to develop a simple, operational method
   that could be used routinely in forensic science casework and that has
   the potential to generate more complete profiles, which would not be
   detected using standard extraction methods on this type of sample. For
   ease of implementation, the process also adheres to standard methods
   with no increase in the cycle number.}},
