@inproceedings{ ISI:000369848800028,
Author = {Ridge, Isabel M. L. and Flory, John F. and Banfield, Steven J.},
Book-Group-Author = {{IEEE}},
Title = {{New European Rope Test Facility}},
Booktitle = {{2014 OCEANS - ST. JOHN'S}},
