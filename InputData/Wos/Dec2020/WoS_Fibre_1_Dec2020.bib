@article{ ISI:000438589000016,
Author = {Sultana, Nadia and Gunning, Sean and Furst, Stephen J. and Garrard,
   Kenneth P. and Dow, Thomas A. and Vinueza, Nelson R.},
Title = {{Direct analysis of textile dyes from trace fibers by automated
   microfluidics extraction system coupled with Q-TOF mass spectrometer for
   forensic applications}},
Journal = {{FORENSIC SCIENCE INTERNATIONAL}},
Year = {{2018}},
Volume = {{289}},
Pages = {{67-74}},
Month = {{AUG}},
Abstract = {{Textile fiber is a common form of transferable trace evidence at the
   crime scene. Different techniques such as microscopy or spectroscopy are
   currently being used for trace fiber analysis. Dye characterization in
   trace fiber adds an important molecular specificity during the analysis.
   In this study, we performed a direct trace fiber analysis method via dye
   characterization by a novel automated microfluidics device (MFD) dye
   extraction system coupled with a quadrupole-time-of-flight (Q-TOF) mass
   spectrometer (MS). The MFD system used an in-house made automated
   procedure which requires only 10 mL of organic solvent for the
   extraction. The total extraction and identification time by the system
   is under 12 min. A variety of sulfonated azo and anthraquinone dyes were
   analyzed from similar to 1 mm length nylon fiber samples. This
   methodology successfully characterized multiple dyes (>= 3 dyes) from a
   single fiber thread. Additionally, it was possible to do dye
   characterization from single fibers with a diameter of similar to 10 mu
   m. The MFD-MS system was used for elemental composition and isotopic
   distribution analysis where MFD-MS/MS was used for structural
   characterization of dyes on fibers. (c) 2018 Elsevier B.V. All rights
   reserved.}},
