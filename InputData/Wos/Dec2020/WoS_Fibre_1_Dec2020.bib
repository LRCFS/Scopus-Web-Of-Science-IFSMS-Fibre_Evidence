@article{ ISI:000291066000020,
Author = {Zieba-Palus, Janina and Was-Gubala, Jolanta},
Title = {{An investigation into the use of micro-Raman spectroscopy for the
   analysis of car paints and single textile fibres}},
Journal = {{JOURNAL OF MOLECULAR STRUCTURE}},
Year = {{2011}},
Volume = {{993}},
Number = {{1-3, SI}},
Pages = {{127-133}},
