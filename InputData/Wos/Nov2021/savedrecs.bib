@article{ WOS:000377823700035,
Author = {Chen, Victor J. and Smith, Gregory D. and Holden, Amanda and Paydar,
   Niloo and Kiefer, Kathleen},
Title = {{Chemical analysis of dyes on an Uzbek ceremonial coat: Objective
   evidence for artifact dating and the chemistry of early synthetic dyes}},
