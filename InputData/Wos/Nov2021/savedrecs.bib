@article{ WOS:000384770600004,
Author = {Spoljaric, Branimira and Popovic, Maja and Crnjac, Josip and Zderic
   Savatovic, Zrinka and Ratko, Martina and Lozancic, Mateja and Jurak,
   Matea and Spoljaric, Igor and Spoljaric, Daniel and Mrsic, Gordan},
Title = {{GLEANING A HUMAN DNA PROFILE FROM TRACE SWABS COLLECTED FROM ANIMAL
   HAIRS}},
Journal = {{ACTA VETERINARIA-BEOGRAD}},
Year = {{2016}},
Volume = {{66}},
Number = {{2}},
Pages = {{187-202}},
Month = {{JUN}},
Abstract = {{Animal hairs are an apt surface for retention of forensic trace
   epithelial samples. The aim of this study was threefold: to evaluate
   different methods of sample collection (moistened and dry swabs) and DNA
   extraction (Chelex (R) 100 method, Qiagen EZ1 (R) DNA Investigator Kit),
   as well as to examine the morphological differences of hair fibres
   between two species (dog, sheep) and their ultimate impact on sample
   collection and processing. Our preliminary findings suggest that the use
   of EZ1 (R) DNA Investigator Kit yields donor DNA profiles of higher
   quality. The results of different sample collection methods have shown
   intraspecific variations that require further investigation. The ability
   of retention and subsequent extraction of trace DNA appears to be
   similar between the two species, despite significant morphological
   differences between their coat hairs.}},
