@article{ WOS:000706155700012,
Author = {Tadano, Wataru and Tanabe, Eishi and Stellhorn, Jens R. and Komaguchi,
   Kenji and Takeuchi, Masaru and Hayakawa, Shinjiro},
Title = {{Feasibility Studies of X-Ray Computed Tomography for Forensic
   Examination of Single Fibers}},
Journal = {{ANALYTICAL SCIENCES}},
