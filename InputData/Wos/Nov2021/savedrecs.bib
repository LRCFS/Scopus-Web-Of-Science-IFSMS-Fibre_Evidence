@article{ WOS:000687303000013,
Author = {Allibe, N. and Paysant, F. and Willeman, T. and Stanke-Labesque, F. and
   Scolan, V and Eysseric, H.},
Title = {{Ocfentanil testing in hair from a fatality case: Comparative analysis of
   a lock of hair versus a single hair fiber}},
Journal = {{FORENSIC SCIENCE INTERNATIONAL}},
Year = {{2021}},
Volume = {{326}},
Month = {{SEP}},
Abstract = {{In clinical and forensic toxicology, hair analysis offers a larger
   window for detecting drug exposure than blood or urine. Drug
   measurements are generally carried out using a segmented lock of hair,
   but few articles report the use of a single hair to document drug
   exposure. Nevertheless, single hair analysis can be very useful,
   particularly if only small amounts of biological matrices are available.
   More data on analyzing new synthetic opioids (NSOs) in hair are needed
   to help interpretation in future cases. In this study, segmental single
   hair analysis is compared with segmental hair lock analysis to document
   an ocfentanil-related death. The hair lock and single hair analyses were
   performed using the LC-MS/MS method after decontamination and
   incubation. Ocfentanil (OcF) concentrations ranged from 42 to 150 pg/mg
   in the segmented hair lock, depending on the segments. The hair lock and
   single hair analyses showed similar results: the highest concentrations
   were measured in the first two centimeters and decreased from root to
   tip. The similar profiles obtained from both the lock of hair and the
   single hair demonstrate the relevance of single hair analysis in cases
   where very few data are available. This article describes OcF
   concentrations in an authentic hair sample after a documented intake of
   this molecule in a fatality. (c) 2021 Elsevier B.V. All rights reserved.}},
