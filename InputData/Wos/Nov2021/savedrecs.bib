Title = {{Hollow fiber Mid-IR spectrometer with UV laser ablation sampling for
   fine spatial resolution of isotope ratios in solids}},
Booktitle = {{QUANTUM SENSING AND NANO ELECTRONICS AND PHOTONICS XVII}},
Series = {{Proceedings of SPIE}},
