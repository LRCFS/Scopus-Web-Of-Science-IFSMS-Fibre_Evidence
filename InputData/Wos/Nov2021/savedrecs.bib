@article{ WOS:000600900300078,
Author = {Kavitha, B. S. and Radhika, N. K. and Gorthi, Sai Siva and Asokan, S.},
Title = {{Etched Fiber Bragg Grating Sensor for Quantification of DNA}},
Journal = {{IEEE SENSORS JOURNAL}},
Year = {{2021}},
Volume = {{21}},
