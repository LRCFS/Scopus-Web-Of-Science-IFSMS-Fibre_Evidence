@article{ WOS:000546249300007,
Author = {Feltraco Lizot, Lilian de Lima and Cezimbra da Silva, Anne Caroline and
   Bastiani, Marcos Frank and Maurer, Tales Fernando and Hahn, Roberta
   Zilles and Perassolo, Magda Susana and Antunes, Marina Venzon and
   Linden, Rafael},
Title = {{Simultaneous Determination of Cocaine and Metabolites in Human Plasma
   Using Solid Phase Micro-Extraction Fiber Tips C18 and UPLC-MS/MS}},
