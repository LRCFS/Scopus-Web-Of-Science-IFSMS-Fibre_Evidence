@article{ WOS:000189353400006,
Author = {Huang, M and Yinon, J and Sigman, ME},
Title = {{Forensic identification of dyes extracted from textile fibers by liquid
   chromatography mass spectrometry (LC-MS)}},
Journal = {{JOURNAL OF FORENSIC SCIENCES}},
