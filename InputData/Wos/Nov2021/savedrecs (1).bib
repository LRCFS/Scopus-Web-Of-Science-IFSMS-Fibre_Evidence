@article{ WOS:000179285100003,
Author = {Coyle, TJ and Robson, R and Bauer, P},
Title = {{Identification of Lyocell using dispersion staining}},
Journal = {{SCIENCE \& JUSTICE}},
Year = {{2002}},
Volume = {{42}},
Number = {{2}},
Pages = {{75-79}},
Month = {{APR-JUN}},
Abstract = {{Lyocell fibres are increasingly used in manufacture of clothes and have
   started to appear in forensic laboratories. It is not easy to
   distinguish Lyocell fibres from other types of regenerated cellulosic
   fibres, particularly where fibres are deeply dyed. Regenerated
   cellulosic fibres share similar birefringence values but Lyocell fibres
   have a refractive index parallel to the long axis of the fibre greater
   than all those within its generic class. Dispersion staining exploits
   this difference. thus enabling Lyocell fibres to be distinguished from
   others within its generic class. The effects of some dyes on the
   application of the technique are noted.}},
