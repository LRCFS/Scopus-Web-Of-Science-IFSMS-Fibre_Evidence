@inproceedings{ WOS:000179396100037,
Author = {Nakazono, T and Kashimura, S and Hara, K and Hayashiba, Y},
Editor = {{Baccino, E}},
Title = {{Combination of DNA-typing and chemical examination for forensic
   investigation of single bloodstain}},
Booktitle = {{16TH MEETING OF THE INTERNATIONAL ASSOCIATION OF FORENSIC SCIENCES}},
Year = {{2002}},
Pages = {{207-210}},
Note = {{16th Meeting of the International-Association-of-Forensic-Sciences,
   MONTPELLIER, FRANCE, SEP 02-07, 2002}},
Organization = {{Int Assoc Forens Sci}},
Abstract = {{To obtain effective findings to criminal investigation, a procedure was
   attempted for bloodstain examination, in which DNA typing was performed
   together with chemical analysis by gas chromatography/mass spectrometry,
   using only single stain. DNA was decontaminated by passing through a
   solid-phase extraction cartridge, and some chemicals were eluted from
   this cartridge. The derivatized chemicals with MBTFA and BSTFA were
   extracted into SPME-fiber for subjecting to GC-MS analysis. DNA-typing
   was identifiable in a blood stain corresponding to 1 ml of blood. Also,
   mass spectra of some biogenetic compounds and methamphetamine were
   detectable in experimentally prepared bloodstains. This procedure is
   useful for practical examination of bloodstain because of high
   sensitivities for both of DNA-typing and chemical analysis.}},
