@article{ WOS:000221769200012,
Author = {Iwai, M and Hattori, H and Arinobu, T and Ishii, A and Kumazawa, T and
   Noguchi, H and Noguchi, H and Suzuki, O and Seno, H},
Title = {{Simultaneous determination of barbiturates in human biological fluids by
   direct immersion solid-phase microextraction and gas chromatography-mass
   spectrometry}},
Journal = {{JOURNAL OF CHROMATOGRAPHY B-ANALYTICAL TECHNOLOGIES IN THE BIOMEDICAL
   AND LIFE SCIENCES}},
