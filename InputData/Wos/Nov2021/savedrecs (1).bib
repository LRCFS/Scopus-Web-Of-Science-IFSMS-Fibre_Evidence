@article{ WOS:000080514600003,
Author = {Wilson, AS and Edwards, HGM and Farwell, DW and Janaway, RC},
Title = {{Fourier transform Raman spectroscopy: Evaluation as a non-destructive
   technique for studying the degradation of human hair from archaeological
   and forensic environments}},
