@article{ WOS:000181290100016,
Author = {Shor, Y and Tsach, T and Vinokurov, A and Glattstein, B and Landau, E
   and Levin, N},
Title = {{Lifting shoeprints using gelatin lifters and a hydraulic press}},
Journal = {{JOURNAL OF FORENSIC SCIENCES}},
Year = {{2003}},
Volume = {{48}},
Number = {{2}},
Pages = {{368-372}},
Month = {{MAR}},
Note = {{4th European Meeting for Shoeprints and Toolmarks Examiners, BERLIN,
   GERMANY, MAY 15-18, 2001}},
Abstract = {{A method for lifting two-dimensional dust footwear marks on rough or
   porous surfaces, such as cardboard or cloth, using a hydraulic press,
   was examined. It was found that exerting pressure on the lifter by the
   press usually improves the quality of the results. When the shoeprints
   were on rough or soft surfaces, the prints transferred to the gelatin
   lifters were better than those obtained by the conventional method. In
   other cases, using the press did not improve the results but was much
   simpler to apply. Based on the results of this study, the hydraulic
   press/gelatin lifter method (the ``press method{''}) is used at the
   authors' laboratory, depending on the surface from which the shoeprint
   is to be lifted. It is the authors' intention to apply the method to
   other surfaces after finding the optimal pressure for surfaces with
   loose fibers.}},
Publisher = {{AMER SOC TESTING MATERIALS}},
Address = {{100 BARR HARBOR DR, W CONSHOHOCKEN, PA 19428-2959 USA}},
Type = {{Article; Proceedings Paper}},
