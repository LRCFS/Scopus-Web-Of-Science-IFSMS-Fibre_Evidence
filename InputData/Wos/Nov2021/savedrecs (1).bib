@article{ WOS:000270526700014,
Author = {Williams, Mary R. and Moody, Chris and Arceneaux, Lei-Ann and Rinke,
   Caitlin and White, Katie and Sigman, Michael E.},
Title = {{Analysis of black writing ink by electrospray ionization mass
   spectrometry}},
