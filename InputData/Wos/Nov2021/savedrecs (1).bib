@article{ WOS:000181350900004,
Author = {Jochem, G and Lehnert, RJ},
Title = {{On the potential of Raman microscopy for the forensic analysis of
   coloured textile fibres}},
Journal = {{SCIENCE \& JUSTICE}},
Year = {{2002}},
Volume = {{42}},
Number = {{4}},
Pages = {{215-221}},
Month = {{OCT-DEC}},
Abstract = {{Raman microspectroscopy was applied to two sets of producer coloured
   (pigmented) synthetic textile fibres to assess its discrimination and
   identification power. The polyacrylnitrile (PAN) and cellulose-viscose
   (CV) fibres were coloured with up to three pigments. Raman and infrared
   spectra were compared and classified according to quality,
   interpretability and extent of discernible features. The study showed
   that under the conditions used, Raman spectra in the visible range
   provided detailed data on the pigments but gave little or no information
   about the polymer. The opposite holds for infrared which was found to be
   well suited for characterising the polymer but failed to provide useful
   data on the pigments. Utilisation of two Raman excitation wavelengths
   (514 and 633 nm) was sufficient for acquiring interpretable good quality
   spectra except for one sample. The results suggest that the
   identification of at least the main pigment component should be feasible
   by Raman.}},
