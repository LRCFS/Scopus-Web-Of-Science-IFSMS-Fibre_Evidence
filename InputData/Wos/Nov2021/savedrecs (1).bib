@inproceedings{ WOS:000245102000019,
Author = {Ricci, Camilla and Chan, K. L. Andrew and Kazarian, Sergei G.},
Editor = {{Lewis, C and Owen, GP}},
Title = {{Fourier transform infrared spectroscopic imaging for the identification
   of concealed drug residue particles and fingerprints}},
