@article{ WOS:000169148700005,
Author = {Xu, X and Leijenhorst, H and Van den Hoven, P and De Koeijer, JA and
   Logtenberg, H},
Title = {{Analysis of single textile fibres by sample-induced isotachophoresis -
   micellar electrokinetic capillary chromatography}},
Journal = {{SCIENCE \& JUSTICE}},
Year = {{2001}},
Volume = {{41}},
Number = {{2}},
Pages = {{93-105}},
Month = {{APR-JUN}},
Abstract = {{Dyes extracted from single textile fibres of a length likely to be
   encountered in forensic investigations were analysed by sample-induced
   isotachophoresis (ITP) and micellar electrokinetic capillary
   chromatography with a large injection volume (60\% of the capillary).
   Dyes extracted from wool, cotton, viscose, polyamide and silk fibres are
   anionic and can be stacked by ITP and separated by micelles. Dyes were
   (de)stacked and separated all on-column. Compared to a normal injection
   volume, a signal enhancement of 190-640 fold and comparable separation
   (efficiency) were achieved. Single fibres of 2-10 mm with medium shades
   of different colours are normally sufficient. The comparison of similar
   single fibres and of fibres from different batches was demonstrated.
   Influences on the reproducibility were discussed. The process of the
   transition from stacking to destacking/micelle-separation was described.}},
