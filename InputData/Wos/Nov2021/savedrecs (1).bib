@article{ WOS:000180350300022,
Author = {Lloyd, JA and Edmiston, PL},
Title = {{Preferential extraction of hydrocarbons from fire debris samples by
   solid phase microextraction}},
Journal = {{JOURNAL OF FORENSIC SCIENCES}},
Year = {{2003}},
Volume = {{48}},
Number = {{1}},
Pages = {{130-134}},
Month = {{JAN}},
Abstract = {{Headspace analysis by extraction/GC-MS is a common method of detecting
   volatile hydrocarbon accelerants in fire debris samples. Solid-phase
   microextraction was tested to determine if there is selective extraction
   of chemically distinct compounds. It was found that both the
   polydimethylsiloxane, (PDMS) and Carboxen/PDMS solid phase
   microextraction fibers show preferential extraction of aliphatic or
   aromatic compounds from the headspace depending on fiber type and
   temperature. The Carboxen/PDMS fiber type showed particular (although
   not exclusive) selectivity for extraction of aromatic hydrocarbons.
   Other experimental considerations of SPME are noted.}},
