@article{ WOS:A1991GQ26000010,
Author = {HARTSHORNE, AW and LAING, DK},
Title = {{AN ABSORPTION STANDARD FOR MICROSPECTROPHOTOMETRY - RESULTS OF A
   COLLABORATIVE EXERCISE}},
Journal = {{FORENSIC SCIENCE INTERNATIONAL}},
Year = {{1991}},
Volume = {{51}},
Number = {{2}},
Pages = {{263-272}},
Month = {{OCT}},
Abstract = {{Matching of casework colour measurements against those in the Home
   Office Forensic Science Service Fibre Data Collection requires that all
   values be compatible. This is best achieved by the standardisation of
   all microspectrophotometers against a common reference sample. As
   commercially available standards are unsuitable for this purpose, a
