@article{ WOS:000085830300010,
Author = {Hall, BJ and Parikh, AR and Brodbelt, JS},
Title = {{Aqueous phase hexylchloroformate derivatization and solid phase
   microextraction: Determination of benzoylecgonine in urine by gas
   chromatography-quadrupole ion trap mass spectrometry}},
Journal = {{JOURNAL OF FORENSIC SCIENCES}},
Year = {{1999}},
Volume = {{44}},
Number = {{3}},
Pages = {{527-534}},
Month = {{MAY}},
Abstract = {{A derivatization/solid phase microextraction (SPME) method for the
   determination of benzoylecgonine in urine was developed. The
   derivatization is conducted directly in 1 mt of urine while sonicating
   for 3 min with 12 mu L of hexyl chloroformate and 70 mu L Of a mixture
   containing acetonitrile:water:hexanol:2-dimethylaminopyridine (5:2:2:1
   v/v), yielding benzoylecgonine hexyl ester (BHE) as the product. After
   the 3 min period, an aliquot of 25(1 mu L is transferred to a vial for
   SPME. After the desired extraction time the 100 mu m
   polydimethylsiloxane SPME fiber was transferred to the GC-E;IS for
   separation and analysis with a quadrupole ion trap mass spectrometer.
   The hexyl chloroformate derivatization and SPME procedures were
   optimized for compatibility and sensitivity. The method was found linear
   for 0.10 to 20.0 mu g/mL (r(2) = 0.999) of benzoylecgonine in urine
   using benzoylecgonine-d(3) as an internal standard (1.5 mu g/mL).
   Intra-day precisions were 8.8 and 6.8\% RSD for 0.30 mu g/mL and 17 mu
   g/mL benzoylecgonine standards in urine (n = 6), respectively. Inter-day
   precision (n = 3) were 3.3\% RSD, indicating good reproducibility. A
   detection limit of 0.03 mu g/mL (S/N = 3) was achieved, thus making the
   SPME method a simplified alternative to SPE for GC-MS confirmation after
   EMIT tests for benzoylecgonine which have a cutoff of 0.30 mu g/mL.
   Quantitative results by SPME and SPE of two clinical urine specimens
   known positive for cocaine by EMIT were in excellent agreement.
   Benzoylecgonine was detected by the derivatization/SPME method in 22 out
   of 22 other urine specimens known positive for cocaine.}},
