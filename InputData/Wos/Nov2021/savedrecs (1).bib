@article{ WOS:000077574400003,
Author = {Liu, JT and Cheng, P and Suzuki, O},
Title = {{Solid-phase microextraction (SPME) of drugs and poisons from biological
   samples}},
Journal = {{FORENSIC SCIENCE INTERNATIONAL}},
Year = {{1998}},
Volume = {{97}},
Number = {{2-3}},
Pages = {{93-100}},
Month = {{NOV 9}},
Abstract = {{Solid-phase microextraction (SPME), a new solvent-free sample
   preparation technique, was invented by C. Arthur and J. Pawliszyn in
   1990. This method mainly was applied for the extraction of volatile and
   semi-volatile organic pollutants in water samples. However, since 1995,
   SPME has been developed to various biological samples, such as whole
   blood, plasma, urine, hair and breath, in order to extract drugs and
   poisons in forensic field. The main advantages of SPME are: high
   sensitivity, solventless, small sample volume, simplicity and rapidity.
   We have reviewed the papers published in recent years about SPME in
   biological samples, and sorted out main experimental conditions, such as
   fibers, matrixes, the extraction approaches and time, as well as the
   acceleration method. We would expect SPME technique to have a promising
   future for toxicological analysis in forensic practice. (C) 1998
   Elsevier Science Ireland Ltd. All rights reserved.}},
