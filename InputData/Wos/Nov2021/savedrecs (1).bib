@article{ WOS:000180011700008,
Author = {Vongsvivut, J and Shilov, SV and Ekgasit, S and Braiman, MS},
Title = {{Symmetrically tapered < 30-mu m-thick quasi-planar germanium waveguides
   as chemical sensors for microanalysis}},
Journal = {{APPLIED SPECTROSCOPY}},
Year = {{2002}},
Volume = {{56}},
Number = {{12}},
Pages = {{1552-1561}},
Month = {{DEC}},
Abstract = {{Symmetrically tapered planar IR waveguides have been fabricated by
   starting with a ZnS coated concave piece of single-crystal Ge, embedding
   it in an epoxide resin as a supporting substrate, and then grinding and
   polishing a planar surface until the thickness at the taper minimum is <
   30 mum. Such tapering is expected to enhance a waveguide's sensitivity
   as an evanescent wave sensor by maximizing the amount of evanescent wave
   energy present at the thinnest part of the waveguide. As predicted by
   theory, the surface sensitivity, i.e., the absorbance signal per
   molecule in contact with the sensing region, increases with decreasing
   thickness of the tapered region even while the total energy throughput
   decreases. The signal-to-noise ratio obtained depends very strongly on
   the quality of the polished surfaces of the waveguides. The surface
   sensitivity is superior to that obtained with a commercial Ge attenuated
   total reflection (ATR) accessory for several types of sample, including
   thin films (< 10 ng) and small volumes (< 1 muL) of volatile solvents.
   By using the waveguides, light-induced structural changes in the protein
   bacteriorhodopsin were observable using samples as small as similar to50
   pmol (similar to1 mug). In addition, the waveguide sensors can reveal
   the surface compositions on a single human hair, pointing to their
   promise as a tool for forensic fiber analysis.}},
