@article{ WOS:000220239300001,
Author = {Vas, G and Vekey, K},
Title = {{Solid-phase microextraction: a powerful sample preparation tool prior to
   mass spectrometric analysis}},
Journal = {{JOURNAL OF MASS SPECTROMETRY}},
