Title = {{Long-term investigation of composite wrapping systems for the
   rehabilitation of reinforced concrete structures}},
Booktitle = {{PROCEEDINGS OF THE SEVENTEENTH (2007) INTERNATIONAL OFFSHORE AND POLAR
   ENGINEERING CONFERENCE, VOL 1- 4, PROCEEDINGS}},
Series = {{International Offshore and Polar Engineering Conference Proceedings}},
