@article{ ISI:000271283100012,
Author = {Howarth, Jennifer and Coulson, Sally and Newton, Angus},
Title = {{Simulating transfer and persistence of a chemical marker powder for
   Lycopodium clavatum spores}},
Journal = {{FORENSIC SCIENCE INTERNATIONAL}},
Year = {{2009}},
Volume = {{192}},
Number = {{1-3}},
Pages = {{72-77}},
Month = {{NOV 20}},
Abstract = {{In this research a chemical marker powder, based on Lycopodium clavatum
   spores, was studied to determine its transfer and persistence on a
   T-shirt. Such chemical marker powders are used to provide evidence that
   a person has handled a covertly marked object, such as a drug package.
   The powder was found to transfer readily between a marked item and the
   person handling it. The powder was found to persist on a T-shirt for up
   to 13 h; however, there was only a very small amount of powder remaining
   at this time. The rate of loss of the L. clavatum spores was found to
   follow a decay curve. The largest decrease in spores from the T-shirt
   was seen in the first 2 h after the marked item had been handled. (C)
   2009 Elsevier Ireland Ltd. All rights reserved.}},
