@article{ ISI:A1997XB58700003,
Author = {Bruschweiler, W and Grieve, MC},
Title = {{A study on the random distribution of a red acrylic target fibre}},
Journal = {{SCIENCE \& JUSTICE}},
Year = {{1997}},
Volume = {{37}},
Number = {{2}},
Pages = {{85-89}},
Month = {{APR-JUN}},
Abstract = {{This project was a target fibre study carried out by the European Fibres
   Group. Thirty-eight participating laboratories in nineteen countries
   searched four hundred and thirty five randomly selected top outer
   garments. The acrylonitrile/vinyl acetate co-polymer target fibre
   matched those in a red acrylic scarf, marketed in nine of those
   countries. The sales figures for the last five years were between 5- and
   10,000 scarves annually. Matching fibres were found on only two
   garments, one fibre per garment. The possible origin of these two fibres
   is discussed. The result showed that the chances of finding a
