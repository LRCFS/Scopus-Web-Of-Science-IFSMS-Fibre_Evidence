@article{ ISI:A1997WE04400006,
Author = {Grieve, MC and Biermann, TW},
Title = {{Wool fibres - Transfer to vinyl and leather vehicle seats and some
   observations on their secondary transfer}},
Journal = {{SCIENCE \& JUSTICE}},
Year = {{1997}},
Volume = {{37}},
Number = {{1}},
Pages = {{31-38}},
Month = {{JAN-MAR}},
Abstract = {{Live tests were performed to assess the transfer of wool fibres to vinyl
   and to leather vehicle seats. Five donor garments were involved. The
   results were compared with those from a simple laboratory shedding test.
   The loss of wool fibres from a vinyl seat due to secondary transfer and
   through vehicle usage was also investigated. Further experiments were
   concerned with the possibility of secondary transfer occurring after a
   change of clothing. Fibre transfer from wool garments was shown to
   depend not only on the nature of the recipient surface but on the
   textile construction of the donor and on the condition of the fibres.
   Live tests seem to provide the most accurate picture of shedding
   behaviour, but, as they are not always practical, suggestions are given
   for optimizing laboratory tests. New aspects of secondary transfer are
   illustrated. Fibre transfer has again been shown to be a complex subject
   in which it can be dangerous to make generalised assumptions.}},
