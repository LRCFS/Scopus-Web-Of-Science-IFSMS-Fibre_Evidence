@article{ ISI:000281920600002,
Author = {Hiroma, Yuki and Hokura, Akiko and Nakai, Izumi},
Title = {{Forensic Identification of Trunk Mat by Trace Element Analysis of Single
   Fiber with Laser Ablation ICP-MS}},
Journal = {{BUNSEKI KAGAKU}},
Year = {{2010}},
Volume = {{59}},
Number = {{9}},
Pages = {{759-769}},
Month = {{SEP}},
Abstract = {{The application of LA-ICP-MS (laser ablation inductively coupled plasma
   mass spectrometry) to the chemical characterization of single trunk mat
   fibers made of PET was investigated in order to establish a forensic
   analytical method for the discrimination of samples of different
   origins. The analytical conditions of LA-ICP-MS equipped with a 213 nm
   Nd : YAG laser were optimized to analyze trace elements, such as Cu, Sb,
   and Ba at ppm levels. A total of 31 samples produced by 7 car
   manufactures in Japan were used for analysis. The concentrations of Li,
   Mg, Al, P, Ca, Ti, Co, Cu, Ge, Nb, Sb, Ta, and Pb were successfully
   measured from a single fiber sample with a diameter of ca. 20 mu m. It
   was possible to discriminate all 31 samples based on the analytical
   results of a single fiber by LA-ICP-MS combined with those of FT-IR and
   SEM-EDS. LA-ICP-MS has good analytical sensitivity, and requires a much
   shorter preparation time and a smaller sample size than any other,
   conventional element analysis methods. This paper demonstrates for the
   first time that this method is practically useful as a powerful tool for
   the forensic identification of a single trunk mat fiber.}},
