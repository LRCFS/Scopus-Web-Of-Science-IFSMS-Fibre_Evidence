@article{ ISI:000302829800039,
Author = {Araujo, Ana Catarina and Song, Yajing and Lundeberg, Joakim and Stahl,
   Patrik L. and Brumer, III, Harry},
Title = {{Activated Paper Surfaces for the Rapid Hybridization of DNA through
   Capillary Transport}},
