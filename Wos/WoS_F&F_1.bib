@article{ ISI:000496946500024,
Author = {Wu, Qunying and Jiang, Yuanying},
Title = {{Almost Sure Convergence for Self-Normalized Products of Sums of Partial
   Sums of rho(-)-Mixing Sequences}},
Journal = {{FILOMAT}},
