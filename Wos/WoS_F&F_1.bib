@article{ ISI:000284387000002,
Author = {Ueta, Ikuo and Saito, Yoshihiro and Teraoka, Kenta and Matsuura, Hisashi
   and Fujimura, Koji and Jinno, Kiyokatsu},
Title = {{Novel Fire Investigation Technique Using Needle Extraction in Gas
   Chromatography}},
Journal = {{ANALYTICAL SCIENCES}},
Year = {{2010}},
Volume = {{26}},
Number = {{11}},
Pages = {{1127-1132}},
Month = {{NOV}},
Abstract = {{A novel fire investigation technique using a needle extraction device
   was studied. Using a polymer particle-packed needle device, air samples
   containing volatile organic compounds (VOCs) generated from fire
   accelerants, gasoline and kerosene were extracted effectively, and
   subsequent gas chromatographic (GC) analyses were successfully carried
   out. Carpet and wood samples were spiked with gasoline and kerosene,
   followed by monitoring of the time-variation profiles of emitted VOCs up
   to 48 h. The fire accelerants were also measured for combusted carpet
   and wood samples, and the applicability of the proposed method to fire
   investigations was confirmed. Even at 48 h after spiking, groups of
   characteristic compounds were clearly observed in the air environments
   near the combusted sample. This method was further applied to the
   determination of VOCs in simulated fires, strongly suggesting the
   applicability of the developed method to real fire investigations.}},
