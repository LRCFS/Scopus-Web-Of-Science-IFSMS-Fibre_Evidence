@article{ ISI:000278294900002,
Author = {Was-Gubala, Jolanta and Grzesiak, Edyta},
Title = {{The kinetics of colour change in textiles and fibres treated with
   detergent solutions Part II - Spectrophotometric measurements}},
Journal = {{SCIENCE \& JUSTICE}},
Year = {{2010}},
Volume = {{50}},
Number = {{2}},
Pages = {{55-58}},
Month = {{JUN}},
Abstract = {{The aim of this study was to assess colour variations that occur in
   several types of textiles and their constituent fibres, resulting from
   the long-term influence of various laundry detergents. A 14-day
   experiment was conducted using blue, red and grey/black cotton, wool,
   acrylic and polyester textiles. The spectrophotometric measurement of
   colour changes in fabric samples and test solutions, as well as the
   microspectrophotometric analysis of colour changes in single fibres were
   described. An evaluation of the observed colour changes from a forensic
   fibre analysis expert's point of view, as well as that of an average
   user/consumer of the textiles and laundry detergents is also provided.
   (C) 2009 Forensic Science Society. Published by Elsevier Ireland Ltd.
   All rights reserved.}},
